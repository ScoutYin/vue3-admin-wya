{
	"name": "@vocli/template",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"dev": "npm run prompt && npm run all",
		"serve": "vite --host",
		"build": "vite build",
		"preview": "vite preview",
		"clean": "rm -rf ./node_modules/.cache ./node_modules/.vite ./dist",
		"prompt": "node ./build/script/prompt.js",
		"lint": "esw src build config --ext .vue,.js,.jsx --color",
		"lint:fix": "npm run lint -- --fix",
		"lint:watch": "npm run lint -- --watch --fix",
		"lint:style": "stylelint --fix 'src/**/*.{vue,css,scss}' --cache --cache-location node_modules/.cache/stylelint/",
		"all": "cross-env NODE_ENV=development npm-run-all --parallel --race --continue-on-error serve",
		"prepare": "husky install"
	},
	"license": "MIT",
	"dependencies": {
		"@babel/runtime": "^7.16.7",
		"@wya/http": "^1.5.0",
		"@wya/ps": "^1.0.3",
		"@wya/qrcode": "^1.0.2",
		"@wya/sass": "^2.0.0",
		"@wya/utils": "^1.6.5",
		"@wya/vc": "^3.0.0-alpha.20",
		"@wya/vcc": "^0.0.25",
		"echarts": "^5.1.2",
		"html2canvas": "^1.0.0",
		"photoswipe": "^4.1.3",
		"quill": "^1.3.7",
		"vue": "^3.2.13",
		"vue-router": "^4.0.12",
		"vuex": "^4.0.2"
	},
	"devDependencies": {
		"@vitejs/plugin-legacy": "^1.5.0",
		"@vitejs/plugin-vue": "^1.3.0",
		"@vitejs/plugin-vue-jsx": "^1.1.7",
		"@vue/compiler-sfc": "^3.2.13",
		"@wya/vue-cli": "^2.0.0-alpha.5",
		"autoprefixer": "^9.8.6",
		"babel-eslint": "^10.1.0",
		"chalk": "4",
		"cp-cli": "^2.0.0",
		"cross-env": "^6.0.3",
		"cssnano": "^4.1.10",
		"eslint": "^8.20.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-babel": "^5.3.1",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-markdown": "^2.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.2.0",
		"eslint-watch": "^7.0.0",
		"fs-extra": "^9.1.0",
		"husky": "^8.0.0",
		"inquirer": "^8.0.0",
		"lint-staged": "^10.5.4",
		"minimist": "^1.2.6",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.14",
		"postcss-html": "^1.5.0",
		"postcss-import": "^14.0.0",
		"postcss-preset-env": "^7.7.2",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"rollup-plugin-visualizer": "^4.2.2",
		"sass": "^1.32.8",
		"shelljs": "^0.8.4",
		"stylelint": "^14.9.1",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-standard-scss": "^5.0.0",
		"stylelint-config-standard-vue": "^1.0.0",
		"stylelint-order": "^4.1.0",
		"vite": "^2.4.4",
		"vue-eslint-parser": "^9.0.3"
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.vue": [
			"eslint --fix",
			"prettier --write",
			"stylelint --fix"
		],
		"*.{css,scss,html}": [
			"stylelint --fix",
			"prettier --write"
		],
		"*.{md,json}": [
			"prettier --write"
		],
		"package.json": [
			"prettier --write"
		]
	}
}
